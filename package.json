		"@types/express": "^5.0.6",
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^24.10.4",
		"@types/pg": "^8.16.0",
		"cross-env": "^10.1.0",
		"drizzle-kit": "^0.31.8",
		"tsc-alias": "^1.8.16",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@taylabs-auth-sdk/express": "github:TayLabs/Auth_Express",
