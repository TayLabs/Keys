{
	"name": "taylabs-keys",
	"version": "1.0.0",
	"description": "",
	"main": "src/server.ts",
	"scripts": {
		"dev": "cross-env NODE_ENV=development tsx src/server.ts",
		"watch": "cross-env NODE_ENV=development tsx watch --include \"./src/**/*\" src/server.ts",
		"db:init": "npx drizzle-kit generate && npx drizzle-kit migrate",
		"docker:up": "docker compose --profile development up -d",
		"docker:start": "docker compose --profile development start",
		"docker:stop": "docker compose --profile development stop",
		"docker:down": "docker compose --profile development down",
		"docker:build": "docker build -t taylabs-keys .",
		"build": "tsc && tsc-alias",
		"start": "node dist/server.ts"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.22.0",
